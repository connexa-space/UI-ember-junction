<div class="card px-0 shadow border-dark">
  <div class="card-header border-0 text-uppercase">
    <i class="fa-light fa-chart-mixed"></i>
    <span class="ms-1">Summary</span>
  </div>
  <div class="card-body">
  	<table class="table">
	  <thead>
	    <tr>
	      <th scope="col">Types</th>
	      <th scope="col">#</th>
	    </tr>
	  </thead>
	  <tbody>
	  	{{#each-in @types as |key type|}}
	    <tr>
	      <td>{{key}}</td>
	      <td>25</td>
	    </tr>
	    {{/each-in}}
	  </tbody>
	</table>
  </div>
</div>
{{yield}}