{{#if @navtitle}}
	<li class="fs-smaller nav-item">
      <LinkTo {{on 'click' @collapseMenu}} class="nav-link text-white ms-3" @route="{{@navroute}}">
      	<i class="{{@iconclass}}"></i>
      	<span class="ps-1">{{@navtitle}}</span>
      </LinkTo>
    </li>
{{else}}
	<li class="nav-item">
		{{yield}}
	</li>
{{/if}}